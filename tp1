#include <stdio.h>
#include <stdlib.h>
# define N 5

typedef string[15];
typedef struct{
    int identifiant;
    char nom[15];
}CLIENT ;
typedef CLIENT tab[];

void saisie(CLIENT *c){
    printf("\n id ? : ");
        scanf("%d", &c->identifiant);
    printf("\n nom ? ");
        scanf("%s", &c->nom);
}

main(int argc, char **argv){
 CLIENT c1,
 tab t;
 saisie (&c1);
 }
